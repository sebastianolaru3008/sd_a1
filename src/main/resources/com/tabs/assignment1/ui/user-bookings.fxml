<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="720.0" prefWidth="1280.0"
            xmlns="http://javafx.com/javafx/17"
            fx:controller="com.tabs.assignment1.ui.UserBookingsView">
    <TableView fx:id="bookingsTableView" layoutX="240.0" layoutY="160.0" prefHeight="400.0"
               prefWidth="800.0">
        <columns>
            <TableColumn fx:id="destinationCol" prefWidth="110.0" text="Destination"/>
            <TableColumn fx:id="nameCol" prefWidth="123.0" text="Name"/>
            <TableColumn fx:id="priceCol" prefWidth="77.0" text="Price"/>
            <TableColumn fx:id="periodCol" prefWidth="259.0" text="Period"/>
            <TableColumn fx:id="detailCol" prefWidth="128.0" text="Details"/>
            <TableColumn fx:id="seatCol" prefWidth="102.0" text="Seats"/>
        </columns>
    </TableView>
    <Text layoutX="484.0" layoutY="102.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Current Bookings">
        <font>
            <Font size="36.0"/>
        </font>
    </Text>
    <Button fx:id="cancelBtn" layoutX="609.0" layoutY="618.0" mnemonicParsing="false"
            onAction="#cancel" text="Cancel"/>
</AnchorPane>
